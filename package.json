{
  "name": "benfords-law",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@hookform/resolvers": "^0.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/rosie": "^0.0.37",
    "@types/styled-components": "^5.1.2",
    "@types/yup": "^0.29.6",
    "axios": "^0.20.0",
    "date-fns": "^2.16.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.7.0",
    "faker": "^5.1.0",
    "fontsource-roboto": "^3.0.3",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.2.13",
    "named-urls": "^2.0.0",
    "notistack": "^1.0.0",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.6.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "rosie": "^2.0.1",
    "styled-components": "^5.1.1",
    "swr": "^0.3.1",
    "typescript": "^4.0.2",
    "yarnhook": "^0.4.3",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --colors --watch --watchAll=false --env=jest-environment-jsdom-sixteen",
    "test:ci": "CI=true react-scripts test --colors --coverage --env=jest-environment-jsdom-sixteen",
    "test:all": "yarn lint && yarn typecheck && yarn test",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src --max-warnings=0",
    "lint:fix": "yarn lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  }
}
